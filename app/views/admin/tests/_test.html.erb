<tr sortable-element>
  <td><%= test.category.title %></td>
  <td sort-by="title"><%= link_to test.title, admin_test_path(test) %></td>
  <td style="text-align: center" sort-by="level"><%= test.level %></td>
  <td style="text-align: center"><%= test.questions.count %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test) %></td>
  <td><%= link_to t('.delete'), admin_test_path(test), method: :delete %></td>
</tr>
