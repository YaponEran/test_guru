<%= form_with model: [:admin, @test], local: true do |test| %>
  <%= render 'shared/errors', resource: @test %>
  <div>
    <%= test.label :title %><br>
    <%= test.text_area :title, id: :test_title, size: '30x3' %>
  </div>
  <div>
    <%= test.label :level %><br>
    <%= test.number_field :level, id: :test_level, in: 0..100 %>
  </div>
  <div>
    <%= test.label :category_id %><br>
    <%= test.collection_select :category_id, Category.all, :id, :title %>
  </div>
  <div>
    <%= test.submit %>
  </div>
<% end %>
